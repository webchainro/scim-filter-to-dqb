{
    "name": "webchainro/scim-search-to-dqb",
    "description": "Takes a SCIM filter string and parses it into a Doctrine QueryBuilder",
    "keywords": ["SCIM", "AST", "parser", "SCIM parser", "SCIM filter parser", "SCIM AST", "DQL", "Doctrine"],
    "type": "library",
    "authors": [
        {
            "name": "Andrei Gorgan",
            "email": "andrei.gorgan@webchain.ro"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webchainro/scim-filter-parser"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webchain\\ScimFilterToDqb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webchain\\ScimFilterToDqb\\Tests\\": "tests/"
        }
    },
    "require": {
        "doctrine/orm": "*",
        "tmilos/scim-filter-parser": "dev-master",
        "psr/http-message": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
